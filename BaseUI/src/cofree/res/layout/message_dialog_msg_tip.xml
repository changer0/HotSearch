<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/message_dialog_msg"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        style="@style/BaseText.T2_C106"
        tools:text="免广告券已生效"
        android:paddingLeft="@dimen/padding_L1"
        android:paddingRight="@dimen/padding_L1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/message_dialog_msg_tips"
        android:layout_marginTop="@dimen/margin_L2"
        app:layout_constraintTop_toBottomOf="@id/message_dialog_msg"
        style="@style/BaseText.T3_C104"
        tools:text="免广告券已生效"
        android:paddingLeft="@dimen/padding_L1"
        android:paddingRight="@dimen/padding_L1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
</androidx.constraintlayout.widget.ConstraintLayout>